   "source": [
    "def pred_bayes_logistic(w_map, S_N, theta):\n",
    "    mu_a = np.dot(w_map.T, theta)\n",
    "    var_a = np.dot(np.dot(theta.T, S_N), theta)\n",
    "    kappa_var = (1 + math.pi*var_a/8)^(-0.5)\n",
    "    x = kappa_var*mu_a\n",
    "    return 1/(1 + np.exp(-x))"
   ]
